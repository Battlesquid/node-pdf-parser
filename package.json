{
    "name": "node-pdf-parser",
    "version": "1.0.3",
    "description": "Library for parsing PDFs using Mozilla's pdf.js",
    "main": "dist/index.js",
    "repository": {
        "url": "git+ssh://git@github.com:Battlesquid/node-pdf-parser.git"
    },
    "author": "Battlesquid <25509915+Battlesquid@users.noreply.github.com>",
    "license": "MIT",
    "scripts": {
        "build": "tsc",
        "build:clean": "pnpm clean && pnpm build",
        "clean": "rimraf dist",
        "reset": "pnpm clean && rimraf node_modules",
        "lint": "eslint src/**",
        "lint:fix": "pnpm lint --fix",
        "neat": "pnpm lint:fix && pnpm pretty",
        "pretty": "prettier . --write",
        "pretty:check": "prettier . --check",
        "test": "vitest"
    },
    "dependencies": {
        "pdfjs-dist": "3.11.174"
    },
    "devDependencies": {
        "@eslint/js": "^9.12.0",
        "@types/node": "^22.13.10",
        "eslint": "^9.12.0",
        "prettier": "^3.3.3",
        "rimraf": "^6.0.1",
        "tsx": "^4.19.3",
        "typescript": "5.5.4",
        "typescript-eslint": "^8.9.0",
        "vitest": "^3.0.8"
    },
    "volta": {
        "node": "22.14.0",
        "pnpm": "10.6.2"
    },
    "files": [
        "dist"
    ],
    "pnpm": {
        "onlyBuiltDependencies": [
            "canvas",
            "esbuild"
        ],
        "patchedDependencies": {
            "pdfjs-dist": "patches/pdfjs-dist.patch"
        }
    },
    "keywords": [
        "pdf",
        "pdf-parser",
        "pdfjs",
        "pdf.js",
        "typescript"
    ]
}
